#!/usr/bin/make -f

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ --with systemd

override_dh_systemd_enable:
	dh_systemd_enable --name=osprey-low-level-control-service.service

override_dh_systemd_start:
	dh_systemd_start --restart-after-upgrade --name=osprey-low-level-control-service.service